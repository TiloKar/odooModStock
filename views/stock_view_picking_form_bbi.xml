<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="stock_view_picking_form_bbi" model="ir.ui.view">
    <field name="name">stock.view.picking.form.bbi</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='origin']" position="after">
        <field name='csv_file'/>
        <button name='ebom_csv' type="object" string="Datei als e-BOM importieren" class="oe_highlight"/>
        <field name='output'/>
      </xpath>
    </field>
  </record>
</odoo>
